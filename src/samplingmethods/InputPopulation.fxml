<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="samplingmethods.FXMLDocumentController">
   <children>
      <TextArea fx:id="samplingFrameArea" editable="false" layoutX="320.0" layoutY="75.0" prefHeight="200.0" prefWidth="200.0" />
      <TextField fx:id="inputPopulationFrame" layoutX="105.0" layoutY="150.0" onKeyPressed="#enterListener" />
      <Button fx:id="enterInputButton" layoutX="154.0" layoutY="188.0" mnemonicParsing="false" onAction="#enterInput" text="Enter" />
      <Label layoutX="121.0" layoutY="58.0" text="Input Sampling Frame" />
      <Button fx:id="proceedSampling" layoutX="400.0" layoutY="297.0" mnemonicParsing="false" onAction="#proceedSamplingFrame" text="Next" />
      <Button fx:id="backFromInputFrame" layoutX="344.0" layoutY="297.0" mnemonicParsing="false" onAction="#returnFromInputFrame" text="Back" />
      <Label fx:id="inputsRemaining" layoutX="118.0" layoutY="125.0" text="Inputs remaining:" />
      <Label fx:id="errorMessage2" layoutX="320.0" layoutY="337.0" prefHeight="17.0" prefWidth="207.0" textFill="#f81c1c">
         <font>
            <Font name="Tahoma" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
